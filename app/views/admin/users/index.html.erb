<h1>Users</h1>
<%= link_to 'New User', new_admin_user_path %>
<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>User Type</th>
      <th>Actions</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.user_type %></td>
        <td><%= link_to 'Details', admin_user_path(user) %></td>
      </tr>
    <% end %>
  </tbody>
</table>


